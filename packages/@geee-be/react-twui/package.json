{
  "name": "@geee-be/react-twui",
  "version": "2.2.2",
  "description": "A comprehensive React UI component library built with Tailwind CSS v4 and Radix UI primitives. Features an intuitive compound component API, full TypeScript support, and accessibility-first design.",
  "keywords": [
    "react",
    "ui",
    "components",
    "tailwindcss",
    "tailwind",
    "radix-ui",
    "typescript",
    "compound-components",
    "accessibility",
    "design-system",
    "ui-library",
    "frontend"
  ],
  "license": "MIT",
  "author": "",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.cts",
  "files": [
    "dist",
    "css",
    "README.md"
  ],
  "scripts": {
    "build": "concurrently \"npm run build:cjs\" \"npm run build:esm\"",
    "build:cjs": "tsup --format cjs --external react",
    "build:esm": "tsup --format esm --external react",
    "build:watch": "concurrently \"npm:build:watch:*\"",
    "build:watch:cjs": "npm run build:cjs -- --watch",
    "build:watch:esm": "npm run build:esm -- --watch",
    "clean": "rimraf .turbo node_modules dist",
    "dev": "npm run build:watch",
    "lint": "npm run lint:biome && npm run lint:types && npm run lint:unused-exports",
    "lint:biome": "biome ci src/",
    "lint:biome:fix": "biome check src/ --write",
    "lint:types": "tsc --noEmit",
    "lint:unused-exports": "ts-unused-exports tsconfig.unused-exports.json --excludePathsFromReport=src/index.ts",
    "storybook": "storybook dev -p 6006 --ci"
  },
  "dependencies": {
    "@geee-be/core": "^1.3.0",
    "@geee-be/react-utils": "workspace:*",
    "@iconicicons/react": "^1.5.1",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/container-queries": "^0.1.1",
    "advanced-cropper": "^0.17.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "color": "^5.0.0",
    "color2k": "^2.0.3",
    "cva": "1.0.0-beta.1",
    "date-fns": "^4.1.0",
    "deepmerge": "^4.3.1",
    "downshift": "^9.0.10",
    "embla-carousel": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "flat": "^6.0.1",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.542.0",
    "react-advanced-cropper": "^0.20.1",
    "react-day-picker": "^9.9.0",
    "react-dropzone-esm": "^15.2.0",
    "react-hook-form": "^7.62.0",
    "react-toastify": "^11.0.5",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "use-debounce": "^10.0.6",
    "usehooks-ts": "^3.1.1",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "^18.0.1",
    "@storybook/addon-docs": "^9.1.4",
    "@storybook/react-vite": "^9.1.4",
    "@swc/core": "^1.13.5",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/react": "^16.3.0",
    "@types/color": "^4.2.0",
    "@types/jest": "^30.0.0",
    "@types/lodash.omit": "^4.5.9",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint-plugin-storybook": "9.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "storybook": "^9.1.4",
    "storybook-dark-mode": "^4.0.2",
    "ts-jest": "^29.4.1",
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0",
    "tailwindcss": ">=4.1"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  }
}
